<h1>Parks List</h1>

<div class="parks-container">
  {{#each parks}}
    <div class="park-item">
      <h2>{{name}}</h2>
      <p><strong>Location:</strong> {{location}}</p>
      <p><strong>Rating:</strong> {{rating}}/5.0</p>
      <a href="/parks/{{id}}" class="view-details-btn">View Details</a>
    </div>
  {{/each}}
</div>


